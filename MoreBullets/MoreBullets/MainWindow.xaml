<Window x:Class="MoreBullets.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MoreBullets"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="525" xmlns:vw="clr-namespace:MoreBullets.Views">
    <Grid>
        <StackPanel Orientation="Horizontal" Height="35" VerticalAlignment="Top" HorizontalAlignment="Center">
            <Button Content="LogBook" MinWidth="100" Click="BtnClickLogBook" />
            <Button Content="About" MinWidth="100" Margin="10,0,0,0" Click="BtnClickAbout"  />
        </StackPanel>
        <Frame x:Name="Main" Margin="0,35,0,0" NavigationUIVisibility="Hidden" />
    </Grid>
</Window>
